package tuples/main

import fmt

nothing = ()

stuff :: (int, (), string)
stuff = (1, nothing, "yey")

pair :: #a -> #b -> (#a, #b)
pair(x, y) = (x, y)

main :: -> ()
main() = {
  fmt.Println(stuff)

  if (1, 2, 3) == (1, 2, 3) then {
    fmt.Println("Equality works.")
  } else {
    fmt.Println("OMG no!")
  }

  fmt.Println(pair(2, "hello") == (2, "hello"))

  nothing
}
