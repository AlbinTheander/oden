package structs/main

import fmt

type Foo = { name string }
type Bar = { foo Foo }

bar = Bar{Foo{"hi!"}}

// Typed by struct fields, not name.
printFooName :: { foo { name string }} -> ()
printFooName(x) = fmt.Println(x.foo.name)

// NOTE: Polymorphic struct fields not supported yet!

main() = {
  printFooName(bar)
}
