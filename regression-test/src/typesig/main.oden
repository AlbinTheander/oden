;; -*- mode: lisp;-*-
(pkg typesig/main)

(import fmt)

(def (identity-no-type-sig x) x)

(: identity-implicit (#a -> #a))
(def (identity-implicit x) x)

(: identity-explicit (forall (#a) (#a -> #a)))
(def (identity-explicit x) x)

(: the-message string)
(def the-message "Type signatures work!")

(def (main)
  (fmt.Println
   (identity-explicit
    (identity-implicit
     (identity-no-type-sig the-message)))))
